<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="         http://java.sun.com/xml/ns/javaee         http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<!-- This descriptor activates the JSF 2.0 Servlet Write your navigation 
		rules here. You are encouraged to use CDI for creating @Named managed beans. -->
	

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/modules/caja/boveda/transaccionBoveda.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/modules/caja/boveda/voucherTransaccionboveda.xhtml</to-view-id>
			<redirect>
				<view-param>
					<name>idtransaccionboveda</name>
					<value>#{transaccionBovedaBean.transaccionboveda.idtransaccionboveda}</value>
				</view-param>
			</redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>retiroInvalido</from-outcome>
			<to-view-id>/error/retiroInvalido.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>invalidTransaction</from-outcome>
			<to-view-id>/error/errorInvalidTransaccionBoveda.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/error/error-page.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
